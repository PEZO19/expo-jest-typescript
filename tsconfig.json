{
  "compilerOptions": {
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-native",
    "paths": {
      "*": [
        "*",
        "*.ios",
        "*.web",
        "*.native",
        "*.android"
      ]
    },
    "noEmit": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
